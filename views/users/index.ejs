<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>About page</title>
  </head>
  <body>
    <%- include('../partials/navbar') %>
    <h1>Users page</h1>
    <p><b>List of Users</b></p>
    <div>
      <% users.map(user => { %>
      <hr />
      <h2><%= user.name %></h2>
      <h5><%= user.job %></h5>
      <h4><%= user.email %></h4>
      <p><%= user.address %></p>
      <hr />
      <% }) %>
    </div>
  </body>
</html>
